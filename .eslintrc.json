{
    "parser": "babel-eslint",
    "overrides": [{
        "files": ["bin/**"],
        "rules": {
            "no-console": "off",
            "no-process-exit": "off"
        }
    }, {
        "files": ["lib/**/*.spec.js"],
        "rules": {
            "node/no-unpublished-require": "off"
        }
    }],
    "extends": [
        "plugin:putout/recommended",
        "plugin:node/recommended"
    ],
    "plugins": [
        "putout",
        "node"
    ]
}
